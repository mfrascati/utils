{
    "name": "mfrascati/utils",
    "description": "Utilities plugin for Entheos",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.3",
        "friendsofcake/crud": "^6.0",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Entheos\\Utils\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entheos\\Utils\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
